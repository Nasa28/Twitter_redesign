<% @follows.each do |user| %>
      <div class = ' justify-content-around m-2 align-items-center'>
        <%= profile_photo(user.followed) %>
        <strong class ="pl-2"><%= link_to  user.followed.full_name.split.map(&:capitalize)*' ',
         user_path(user.followed), class: "text-decoration-none p-1"%></strong>
      </div>
    <% end %>