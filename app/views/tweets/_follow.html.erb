  <p>WHO TO FOLLOW</p>
        <% @users.each do |user| %>
        <%return if signed_in%>
        <%unless @follows.include?(user.id)%>
      <div class = ' justify-content-around mt-2 align-items-center'>
        <%= profile_photo(user) %>
        <strong class ="pl-2"><%= link_to  user.full_name.split.map(&:capitalize)*' ',
         user_path(user), class: "text-decoration-none"%></strong>
      
      </div>
      <%end%>
    <% end %>